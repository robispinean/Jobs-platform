{
    "env": {
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off"
    },
    "settings": {
      "import/extensions": "off",
      "import/resolver": {
        "node": {
          "extensions": [
            ".js"
          ]
        }
      }
    }
  }
  